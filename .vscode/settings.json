{
  "cSpell.enableFiletypes": [
    "css",
    "dotenv",
    "html",
    "javascript",
    "json",
    "markdown",
    "plaintext",
    "text",
    "typescript",
    "xml"
  ],
  "cSpell.ignorePaths": [
    "**/.env*",
    "**/bower_components/**",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/yarn.lock",
    "**/.git/objects/**",
    "src/www/messages/*.json"
  ],
  "cSpell.words": [
    "Apks",
    "Cygwin",
    "Cygwinâ€™s",
    "KEYALIAS",
    "KEYPASS",
    "Pcdv",
    "STOREPASS",
    "Shadowsocks",
    "Transpiles",
    "Transpiling",
    "armv",
    "beepass",
    "beepassvpn",
    "gulpfile",
    "gulpif",
    "gutil",
    "osascript",
    "posthtml",
    "rsync",
    "rsynced",
    "s",
    "shadowsocksconfig",
    "splashscreen",
    "swiper",
    "transpiled",
    "uuidv",
    "webcomponentsjs",
    "webintent",
    "xcode",
    "xcodeproj"
  ],
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "files.associations": {
    "Dockerfile*": "dockerfile"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/licenses.txt": true,
    "**/yarn.lock": true,
    "**/*.code-search": true,
    "**/*.csv": true,
    "**/*.markdown": true,
    "**/*.xcodeproj": true,
    "apple/xcode/**/*": true,
    "cordova-plugin-outline/**/*": true,
    "resources/master_messages.json": true,
    "src/www/messages/*.json": true,
    "third_party/**/*": true,
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
}
